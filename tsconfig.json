{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019"],
    "module": "CommonJS",
    "allowJs": true,
    "checkJs": true,
    "sourceMap": false,
    "removeComments": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "outDir": "./dist/"
  },
  "typeAcquisition": {
    "enable": true
  },
  "include": ["src/app.ts", "./src/*"]
}
